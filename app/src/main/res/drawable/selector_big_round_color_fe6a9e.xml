<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools" tools:ignore="MissingDefaultResource">
    <item android:state_selected="true">
        <shape>
            <solid android:color="@color/color_99FE649A" />
            <corners android:radius="90dp" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/color_99FE649A" />
            <corners android:radius="90dp" />
        </shape>
    </item>
    <item android:state_checked="true">
        <shape>
            <solid android:color="@color/color_99FE649A" />
            <corners android:radius="90dp" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/gray_disable" />
            <corners android:radius="90dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/color_FE6A9E" />
            <corners android:radius="90dp" />
        </shape>
    </item>
</selector>